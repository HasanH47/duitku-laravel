{
  "name": "duitku/laravel",
  "description": "Duitku Payment Gateway for Laravel",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Hasan H",
      "email": "me@hasanh.dev"
    }
  ],
  "require": {
    "php": "^8.2",
    "illuminate/support": "^10.0|^11.0|^12.0",
    "illuminate/http": "^10.0|^11.0|^12.0",
    "guzzlehttp/guzzle": "^7.8"
  },
  "require-dev": {
    "pestphp/pest": "^2.34|^3.0|^4.0",
    "pestphp/pest-plugin-laravel": "^2.0|^3.0|^4.0",
    "orchestra/testbench": "^8.0|^9.0|^10.0",
    "phpstan/phpstan": "^1.10",
    "larastan/larastan": "^2.0",
    "laravel/pint": "^1.27"
  },
  "autoload": {
    "psr-4": {
      "Duitku\\Laravel\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Duitku\\Laravel\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Duitku\\Laravel\\DuitkuServiceProvider"
      ],
      "aliases": {
        "Duitku": "Duitku\\Laravel\\Facades\\Duitku"
      }
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
